{
  "name": "wisdo",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "ts-node src/bin/www.ts",
    "start:dev": "cross-env DEBUG=wisdo:* nodemon -T src/bin/www.ts",
    "test": "run-p test:unit test:integration",
    "test:integration": "docker-compose up -d db-test && cross-env NODE_ENV=test jest --runInBand --verbose=false -c jest.config.js ./test/integration",
    "test:unit": "cross-env NODE_ENV=test jest -c jest.config.js ./test/unit",
    "seed": "ts-node src/scripts/seed.ts",
    "docker:startd": "docker-compose up -d",
    "docker:start": "docker-compose up",
    "docker:seed": "docker-compose run api npm run seed"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.0",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-cache": "^5.1.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "require-directory": "^2.1.1",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0"
  },
  "devDependencies": {
    "@trendyol/jest-testcontainers": "^2.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.6",
    "@types/faker": "^5.5.7",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/require-directory": "^2.1.2",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "npm-run-all": "^4.1.5",
    "sinon": "^11.1.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
